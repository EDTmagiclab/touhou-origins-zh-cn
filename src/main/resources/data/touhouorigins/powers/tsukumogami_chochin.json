{
  "type": "apoli:multiple",
  "soulbind_callback": {
    "type": "apoli:action_on_callback",
    "entity_action_chosen": {
      "type": "apoli:execute_command",
      "command": "give @s end_rod{Unbreakable:1,display:{Name:'{\"text\":\"Chōchin\"}',Lore:['{\"text\":\"Lore\"}']},tsukumogami_bound:1} 1"
    },
    "entity_action_lost": {
      "type": "apoli:execute_command",
      "command": "clear @s end_rod{Unbreakable:1,display:{Name:'{\"text\":\"Chōchin\"}',Lore:['{\"text\":\"Lore\"}']},tsukumogami_bound:1}"
    },
    "entity_action_respawned": {
      "type": "apoli:and",
      "actions": [
        {
          "type": "apoli:execute_command",
          "command": "clear @s end_rod{Unbreakable:1,display:{Name:'{\"text\":\"Chōchin\"}',Lore:['{\"text\":\"Lore\"}']},tsukumogami_bound:1}"
        },
        {
          "type": "apoli:delay",
          "ticks": 1,
          "action": {
            "type": "apoli:execute_command",
            "command": "give @s end_rod{Unbreakable:1,display:{Name:'{\"text\":\"Chōchin\"}',Lore:['{\"text\":\"Lore\"}']},tsukumogami_bound:1} 1"
          }
        }
      ]
    }
  },
  "glowing_self": {
    "type": "apoli:action_over_time",
    "entity_action": {
      "type": "apoli:and",
      "actions": [
        {
          "type": "origins:apply_effect",
          "effect": {
            "effect": "minecraft:glowing",
            "duration": 100,
            "show_particles": false
          }
        }
      ]
    },
    "rising_action": {
